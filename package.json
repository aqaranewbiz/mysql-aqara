{
  "name": "@aqaranewbiz/mysql-aqaranewbiz",
  "version": "1.0.0",
  "description": "MySQL MCP server for Smithery with one-click installation",
  "main": "mcp_server.py",
  "bin": {
    "mcp-mysql-server": "./run.js"
  },
  "scripts": {
    "start": "python mcp_server.py",
    "install": "npm install -g",
    "setup": "npm install && chmod +x run.js",
    "postinstall": "chmod +x run.js"
  },
  "mcp": {
    "type": "local",
    "language": "python",
    "command": "python mcp_server.py"
  },
  "dependencies": {
    "mysql-connector-python": "^8.0.33"
  },
  "keywords": [
    "smithery",
    "mcp",
    "mysql",
    "database"
  ],
  "author": "Aqara Newbiz",
  "license": "MIT"
} 